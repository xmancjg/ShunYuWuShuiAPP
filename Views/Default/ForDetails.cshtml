@{
    Layout = null;
}

<!-- 瞬时流量S01 -->
<li class="s-detail-li"><span class="run"><i>@Model.ssll.ToString("F3")</i><br/><font>m³/h</font><p>瞬时流量</p></span></li>
<!-- 累计流量S01 -->
<li class="s-detail-li"><span class="run"><i>@Model.ljll</i><br/><font>m³</font><p>累计流量</p></span></li>
<!-- COD实时检测S01 -->
<li class="s-detail-li"><span class="run"><i>@Model.cod_ss</i><p>COD值</p></span></li>
<!-- PH实时检测S01 -->
<li class="s-detail-li"><span class="run"><i class="phnum">@Model.ph_ss</i><p>PH值</p></span></li>
<!-- 氨氮值 -->
<li class="s-detail-li"><span class="run"><i>@Model.ad_ss</i><p>氨氮值</p></span></li>
<!-- 液位反馈S01 -->
<li class="s-detail-li"><span class="run"><i class="ywnum">@Model.yw</i><br /><font>m</font><p>液位值</p></span></li>



<!-- 一号MBR泵 -->
@if (Model.mbr1_run == 1)
{
    <li class="s-detail-li run"><span><i>运行</i><p>一号MBR泵</p></span></li>
}
else
{
    <li class="s-detail-li stop"><span><i>停止</i><p>一号MBR泵</p></span></li>
}

<!-- 一号回流泵 -->
@if (Model.hlb1_fault == 0 & Model.hlb1_run == 1)
{
    <li class="s-detail-li run"><span><i>运行</i><p>一号回流泵</p></span></li>
}
else if (Model.hlb1_fault == 0 & Model.hlb1_run == 0)
{
    <li class="s-detail-li stop"><span><i>停止</i><p>一号回流泵</p></span></li>
}
else
{
    <li class="s-detail-li alarm"><span><i>故障</i><p>一号回流泵</p></span></li>
}

