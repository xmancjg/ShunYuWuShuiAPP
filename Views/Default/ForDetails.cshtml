<script>
    function PH() {
        var ph = $(".phnum")[0].innerHTML;
        var yw = $(".ywnum")[0].innerHTML;
        //alert(ph);
        $(".phnum")[0].innerHTML = (ph - 0) / 100;
        $(".ywnum")[0].innerHTML = (yw - 0) / 1000;
    }
    PH();

</script>

